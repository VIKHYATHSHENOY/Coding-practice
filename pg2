class pg2 {
    public static void main(String[] args) {
       char ch='t';
       if(Character.isLetter(ch)){
           if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
           {
               System.out.println("Vovel");
           }
           else
           {
               System.out.println("Consonent");
           }
       }
    }
}